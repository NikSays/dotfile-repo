[better-osd]
horizontal=45
size=18
vertical=41

[caffeine]
control-nightlight=true

[clipboard-indicator]
clear-history=['<Primary><Alt>x']
disable-down-arrow=true
history-size=50
next-entry=['<Primary><Alt>v']
prev-entry=['<Primary><Shift><Alt>v']
toggle-menu=['<Primary><Alt>c']

[lockkeys]
style='show-hide'

[ncom/github/hermes83/compiz-alike-windows-effect]
skip-frames-before-spring-start=8.0

[panel-osd]
test-notification=false
x-pos=100.0

[user-theme]
name='Matcha-dark-sea'
